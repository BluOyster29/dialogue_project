<?xml version="1.0" encoding="utf-8"?>

<vxml version="2.1" xmlns="http://www.w3.org/2001/vxml"
      xmlns:voxeo="http://community.voxeo.com/xmlns/vxml">

    <form id="question_one">
        <field name="level_one">
            <prompt>
                Repeat after me.
                She sells sea shells on the sea shore.
            </prompt>
        </field>
        <filled>
          <prompt>
	    You said: <value expr="level_one$.utterance"/>,
	    with the interpretation <value expr="level_one$.interpretation"/>
	    and with a confidence of <value expr="level_one$.confidence"/>
	    <break time ="2s"/>
	  </prompt>
	  <goto next="#commands"/>
	</filled>
    </form>
</vxml>
